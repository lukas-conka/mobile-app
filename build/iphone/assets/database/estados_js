function getTableEstados(){return"tb_estados"}function dbLoad(){var e=Ti.Database.open(Ti.App.Properties.getString(DATABASE_FILE));return e}function createEstados(){db=dbLoad(),db.execute("CREATE TABLE IF NOT EXISTS "+getTableEstados()+"(\n		id INTEGER PRIMARY KEY, es_id INTEGER, es_nome TEXT, es_sigla TEXT		\n		);"),db.close()}function dropEstados(){db=dbLoad(),db.execute("DROP TABLE IF EXISTS "+getTableEstados()),db.close()}function insertEstados(e,i,t){db=dbLoad(),db.execute("INSERT INTO "+getTableEstados()+" (\n		es_id, es_nome, es_sigla) \n	VALUES (?,?,?)",e,i,t),db.close()}function selectallEstados(){db=dbLoad();var e=db.execute("SELECT * FROM "+getTableEstados());return"android"==Ti.Platform.osname&&db.close(),e}function processEstados(e){dropEstados(),createEstados();for(var i=JSON.parse(e),t=0;t<i.length;t++){var o=i[t].es_id,a=i[t].es_nome,l=i[t].es_sigla;insertEstados(o,a,l)}}