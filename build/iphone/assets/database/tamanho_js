function getTableTamanho(){return"tb_tamanhos"}function dbLoad(){var e=Ti.Database.open(Ti.App.Properties.getString(DATABASE_FILE));return e}function createTamanho(){db=dbLoad(),db.execute("CREATE TABLE IF NOT EXISTS "+getTableTamanho()+"(\n		id INTEGER PRIMARY KEY, tmh_id INTEGER, tmh_nome TEXT, ep_id INTEGER\n		);"),db.close()}function dropTamanho(){db=dbLoad(),db.execute("DROP TABLE IF EXISTS "+getTableTamanho()),db.close()}function insertTamanho(e,i,t){db=dbLoad(),db.execute("INSERT INTO "+getTableTamanho()+" (\n		tmh_id, tmh_nome, ep_id) \n	VALUES (?,?,?)",e,i,t),db.close()}function selectallTamanho(){db=dbLoad();var e=db.execute("SELECT * FROM "+getTableTamanho());return"android"==Ti.Platform.osname&&db.close(),e}function processTamanho(e){dropTamanho(),createTamanho();for(var i=JSON.parse(e),t=0;t<i.length;t++)if(0==i[t].tmh_excluido&&0==i[t].tmh_visualizar){var o=i[t].tmh_id,a=i[t].tmh_nome,l=i[t].ep_id;insertTamanho(o,a,l)}}