function getTableEmailEmpresa(){return"tb_email_emp"}function dbLoad(){var e=Ti.Database.open(Ti.App.Properties.getString(DATABASE_FILE));return e}function createEmailEmpresa(){db=dbLoad(),db.execute("CREATE TABLE IF NOT EXISTS "+getTableEmailEmpresa()+"(\n		id INTEGER PRIMARY KEY, ee_id INTEGER, ee_email TEXT, ee_grupo INTEGER, ee_nome TEXT, ep_id INTEGER, ee_cargo TEXT, ee_recebe_pedido INTEGER, ee_recebe_credito INTEGER, fk_marcas INTEGER\n		);"),db.close()}function dropEmailEmpresa(){db=dbLoad(),db.execute("DROP TABLE IF EXISTS "+getTableEmailEmpresa()),db.close()}function insertEmailEmpresa(e,i,t,o,a,l,r,_,d){db=dbLoad(),db.execute("INSERT INTO "+getTableEmailEmpresa()+" (\n		ee_id, ee_email, ee_grupo, ee_nome, ep_id, ee_cargo, ee_recebe_pedido, ee_recebe_credito, fk_marcas) \n	VALUES (?,?,?,?,?,?,?,?,?)",e,i,t,o,a,l,r,_,d),db.close()}function selectallEmailEmpresa(){db=dbLoad();var e=db.execute("SELECT * FROM "+getTableEmailEmpresa());return"android"==Ti.Platform.osname&&db.close(),e}function processEmailEmpresa(e){dropEmailEmpresa(),createEmailEmpresa();for(var i=JSON.parse(e),t=0;t<i.length;t++){var o=i[t].ee_id,a=i[t].ee_email,l=i[t].ee_grupo,r=i[t].ee_nome,_=i[t].ep_id,d=i[t].ee_cargo,s=i[t].ee_recebe_pedido,n=i[t].ee_recebe_credito,c=i[t].fk_marcas;insertEmailEmpresa(o,a,l,r,_,d,s,n,c)}}