function getTableGrupoCliente(){return"tb_grupo_cliente"}function dbLoad(){var e=Ti.Database.open(Ti.App.Properties.getString(DATABASE_FILE));return e}function createGrupoCliente(){db=dbLoad(),db.execute("CREATE TABLE IF NOT EXISTS "+getTableGrupoCliente()+"(\n		id INTEGER PRIMARY KEY, gc_id INTEGER, fk_cli_principal INTEGER, fk_cli_vinculado INTEGER,\n		ep_id INTEGER\n		);"),db.close()}function dropGrupoCliente(){db=dbLoad(),db.execute("DROP TABLE IF EXISTS "+getTableGrupoCliente()),db.close()}function insertGrupoCliente(e,i,t,o){db=dbLoad(),db.execute("INSERT INTO "+getTableGrupoCliente()+" (\n		gc_id, fk_cli_principal, fk_cli_vinculado, ep_id) \n	VALUES (?,?,?,?)",e,i,t,o),db.close()}function selectallGrupoCliente(){db=dbLoad();var e=db.execute("SELECT * FROM "+getTableGrupoCliente());return"android"==Ti.Platform.osname&&db.close(),e}function processGrupoCliente(e){dropGrupoCliente(),createGrupoCliente();for(var i=JSON.parse(e),t=0;t<i.length;t++){var o=i[t].gc_id,a=i[t].fk_cli_principal,l=i[t].fk_cli_vinculado,r=i[t].ep_id;insertGrupoCliente(o,a,l,r)}}