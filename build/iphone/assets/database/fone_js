function getTableFone(){return"tb_fone"}function dbLoad(){var e=Ti.Database.open(Ti.App.Properties.getString(DATABASE_FILE));return e}function createFone(){db=dbLoad(),db.execute("CREATE TABLE IF NOT EXISTS "+getTableFone()+"(\n		id INTEGER PRIMARY KEY, fn_id INTEGER, fn_fone TEXT, fn_ramal INTEGER, fn_contato TEXT, fk_rp TEXT, fk_tipo INTEGER, ep_id INTEGER\n		);"),db.close()}function dropFone(){db=dbLoad(),db.execute("DROP TABLE IF EXISTS "+getTableFone()),db.close()}function insertFone(e,i,t,o,a,l,r){db=dbLoad(),db.execute("INSERT INTO "+getTableFone()+" (\n		fn_id, fn_fone, fn_ramal, fn_contato, fk_rp, fk_tipo, ep_id) \n	VALUES (?,?,?,?,?,?,?)",e,i,t,o,a,l,r),db.close()}function selectallFone(){db=dbLoad();var e=db.execute("SELECT * FROM "+getTableFone());return"android"==Ti.Platform.osname&&db.close(),e}function processFone(e){dropFone(),createFone();for(var i=JSON.parse(e),t=0;t<i.length;t++){var o=i[t].fn_id,a=i[t].fn_fone,l=i[t].fn_ramal,r=i[t].fn_contato,_=i[t].fk_rp,d=i[t].fk_tipo,s=i[t].ep_id;insertFone(o,a,l,r,_,d,s)}}