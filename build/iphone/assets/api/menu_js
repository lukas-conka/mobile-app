function menuSelection(e){switch(e){case 0:goToSelecionaCliente();break;case 1:goToCredito();break;case 2:var i=Ti.App.Properties.getString(CURRENT_SOFTWARE);Ti.API.info("software="+i),1==i?goToCatalogo():goToMarca();break;case 3:goToPedidos();break;case 4:goToTransportadora();break;case 5:var t=Ti.App.Properties.getList(SELECTED_CLIENTS);if(t.length>0)goToNovoCliente();else{var o=Ti.UI.createAlertDialog({message:"É necessário a seleção de um cliente ou mais para seguir em diante!",title:"Selecionar cliente!:"});o.show()}break;case 6:goToFuncoes();break;case 7:goToEmail();break;case 8:goToTelaInicial();break;case 9:goToMarca();break;case 10:goToSair()}}function goToSelecionaCliente(){var e=Alloy.createController("seleciona_cliente").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToCredito(){var e=Alloy.createController("credito").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToMarca(){var e=Alloy.createController("marca").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToCatalogo(){var e=Alloy.createController("categorias").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToPedidos(){var e=Alloy.createController("pedidos").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToTransportadora(){var e=Alloy.createController("transportadora").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToNovoCliente(){var e=Alloy.createController("novo_cliente").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToFuncoes(){var e=Alloy.createController("funcao").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToTelaInicial(){var e=Alloy.createController("index").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToEmail(){var e=Alloy.createController("email").getView();e.open({fullscreen:!0,navBarHidden:!0})}function goToSair(){var e=Alloy.createController("index").getView();e.open({fullscreen:!0,navBarHidden:!0})}